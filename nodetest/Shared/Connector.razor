@using nodetest.ViewModels


<line stroke="red" stroke-width="2" x1="@X1" y1="@Y1" x2="@X2" y2="@Y2"></line> 

@code {
    
    
    [EditorRequired, Parameter]
    public ConnectionViewModel Model { get; set; }

    private int X1 => Model.FromVector.X;
    private int Y1 => Model.FromVector.Y;

    private int X2 => Model.ToVector.X;
    private int Y2 => Model.ToVector.Y;
}


